# Aseg칰rate de que las longitudes de k_points y W_r_k sean iguales
plot(k_points, W_r_k, type = "l", col = "red",
main = paste("Realizaci칩n del Proceso W^{(r)}_k, con r =", r, "y N =", N),
ylab = "W_r_k", xlab = "k")
}
}
